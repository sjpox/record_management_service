generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
}

model Vouchers {
  uuid String @id @db.VarChar(30)
  voucherNumber String @db.VarChar(20)

  @@map("vouchers")
}